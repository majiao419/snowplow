{
	"$schema": "http://json-schema.org/schema#",
	"description": "Schema for a link click event",
	"type": "object",
	"properties": {
		"element_id": {
			"type": "string"
		},
		"element_classes": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"element_target": {
			"type": "string"
		},
		"target_url": {
			"type": "string",
			"minLength": 1
		}
	},
	"required": ["target_url"],
	"additionalProperties": false
}
